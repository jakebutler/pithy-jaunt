# Test Environment Variables
# Copy this to .env.test and fill in with test credentials
# You can copy values from your .env file, but use a separate test Supabase project and Convex deployment

# Supabase Test Project (REQUIRED for E2E tests)
# Create a separate Supabase project for testing to avoid affecting production data
NEXT_PUBLIC_SUPABASE_URL=your_test_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_test_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_test_supabase_service_role_key

# Convex Test Deployment (REQUIRED for E2E tests)
# Create a separate Convex deployment for testing
CONVEX_DEPLOYMENT=your_test_convex_deployment_url
NEXT_PUBLIC_CONVEX_URL=your_test_convex_url

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# GitHub Test Token (REQUIRED for repository connection tests)
# Create a GitHub token with 'repo:public_repo' scope for testing
GITHUB_TOKEN=your_test_github_token

# External Services (optional for E2E tests, not required for smoke tests)
OPENAI_API_KEY=your_test_openai_key
ANTHROPIC_API_KEY=your_test_anthropic_key
MODEL_PROVIDER=openai
MODEL_NAME=gpt-4o

# Test Configuration
TEST_USER_EMAIL_PREFIX=test-user
TEST_CLEANUP_ENABLED=true




