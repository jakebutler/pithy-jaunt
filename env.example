# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Convex
CONVEX_DEPLOYMENT=your_convex_deployment_url
NEXT_PUBLIC_CONVEX_URL=your_convex_url

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# GitHub
GITHUB_TOKEN=your_github_token
# Required scopes: repo, workflow
# For webhook signature verification (optional but recommended)
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# LLM Providers
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key
MODEL_PROVIDER=openai
MODEL_NAME=gpt-4o

# External Services
DAYTONA_API_KEY=your_daytona_key
DAYTONA_API_URL=https://app.daytona.io/api
DAYTONA_TARGET=us
DAYTONA_SNAPSHOT_NAME=pithy-jaunt-dev
# Docker image name for the snapshot (used by SDK)
DAYTONA_IMAGE_NAME=butlerjake/pithy-jaunt-daytona:v1.0.0
# Use TypeScript SDK (default: true, set to false to use REST API)
DAYTONA_USE_SDK=true
# Use GitHub Actions instead of REST API (fallback if SDK fails)
DAYTONA_USE_GITHUB_ACTIONS=false
# GitHub repository for Actions workflow (if using GitHub Actions)
GITHUB_REPO_OWNER=jakebutler
GITHUB_REPO_NAME=pithy-jaunt
GALILEO_API_KEY=your_galileo_key
CODERABBIT_API_KEY=your_coderabbit_key
RESEND_API_KEY=your_resend_key

# Context7 MCP (optional - for library documentation)
# Get your API key at https://context7.com/dashboard
# Can be used without API key with rate limits
CONTEXT7_API_KEY=your_context7_api_key

# Configuration
LOG_RETENTION_DAYS=7
WORKSPACE_CLEANUP_ENABLED=true
WORKSPACE_MAX_CONCURRENT=100
MAX_CONCURRENT_USERS=10

